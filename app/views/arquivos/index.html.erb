<p id="notice"><%= notice %></p>

<h1>Dados Importados</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>Token</th>
      <th>Data Admiss√£o</th>
      <th>Saldo</th>
    </tr>
  </thead>

  <tbody>
    <% @arquivos.each do |arquivo| %>
      <tr>
        <td><%= "#{ arquivo.name } #{ arquivo.surname }" %></td>
        <td><%= arquivo.email %></td>
        <td><%= arquivo.token %></td>
        <td><%= arquivo.admission_date.strftime('%d/%m/%Y') %></td>
        <td><%= number_to_currency(arquivo.available_amount, locale: :"pt-BR") %></td>
      </tr>
    <% end %>
  </tbody>
</table>

